{
  "cli": {
    "version": ">= 6.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "simulator": {
      "ios": {
        "workflow": "managed",
        "buildType": "simulator"
      }
    },
    "production": {
      "ios": {
        "workflow": "managed",
        "buildType": "app-store"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

